var express = require('express');
var router = express.Router();
var $ = require('../service/service<%=Module%>');
// -- custom
/**
 * Auto generate RESTful url routes.
 *
 * URL routes:
 *
 *  GET    /<%=module%>[/]        => $.list()
 *  GET    /<%=module%>/new       => $.new()
 *  GET    /<%=module%>/:id       => $.show()
 *  GET    /<%=module%>/edit/:id  => $.edit()
 *  POST   /<%=module%>[/]        => $.create()
 *  PATCH  /<%=module%>/:id       => $.update()
 *  DELETE /<%=module%>/:id       => $.destroy()
 *
 */


router.get('/new', $.new);
router.get('/edit/:id', $.edit);

router.route('/')
    .get($.list)
    .post($.create);

router.route('/:id')
    .patch($.update)
    .get($.show)
    .delete($.destroy);
module.exports = router;